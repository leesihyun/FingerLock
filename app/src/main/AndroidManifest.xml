<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.seahyun.fingerlock">

    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="lockscreendemo"></activity>
        <activity android:name=".TouchTabSettingActivity"></activity>
        <activity android:name=".TouchTabPreview" android:theme="@android:style/Theme.Dialog"></activity>
        <activity
            android:name=".ApplicationSelectionActivity"
            android:theme = "@android:style/Theme.Dialog"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".TouchTabFourActivity"></activity>
        <activity android:name=".TouchTabFiveActivity"></activity>
        <activity android:name=".TouchTabSixActivity"></activity>
        <service android:name="SimpleService"></service>
    </application>

</manifest>

