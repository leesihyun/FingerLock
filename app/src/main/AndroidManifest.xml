<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.seahyun.fingerlock">

    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".lockscreendemo" />
        <activity android:name=".TouchTabSettingActivity" />
        <activity
            android:name=".TouchTabPreview"
            android:theme="@android:style/Theme.Dialog" />
        <activity
            android:name=".ApplicationSelectionActivity"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Dialog"></activity>
        <activity android:name=".PinAppRegister" />
        <activity android:name=".PinPassword" />
        <activity android:name=".PinLock" />
        <activity android:name=".PinSettingActivity" />
        <activity android:name=".RE_PinPassword" />
        <activity android:name=".TouchTabFourActivity" />
        <activity android:name=".TouchTabFiveActivity" />
        <activity android:name=".TouchTabSixActivity" />
        <activity android:name=".TouchTabFourPassword" />
        <activity android:name=".RE_TouchTabFourPassword" />
        <activity android:name=".TouchTabFivePassword" />
        <activity android:name=".RE_TouchTabFivePassword" />
        <activity android:name=".TouchTabSixPassword" />
        <activity android:name=".RE_TouchTabSixPassword" />

        <service android:name=".SimpleService" />

        <activity android:name=".PinSettingActivity"></activity>
    </application>

</manifest>